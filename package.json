    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@testing-library/react": "14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/ms": "^0.7.34",
