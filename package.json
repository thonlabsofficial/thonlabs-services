{
  "name": "thonlabs-services",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint": "eslint \"src/**/*.ts\"",
    "prisma": "prisma",
    "prisma:generate": "prisma generate --schema=./src/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=./src/database/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=./src/database/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./src/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/database/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./src/database/prisma/schema.prisma",
    "email:dev": "email dev --dir ./src/packages/react-email --port 6591",
    "email:export": "email export --dir ./src/packages/react-email"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@nestjs/common": "^10.4.17",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/throttler": "^5.1.1",
    "@prisma/client": "^5.8.1",
    "@react-email/components": "1.0.1",
    "@react-email/render": "^0.0.12",
    "axios": "^1.12.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.3.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.1",
    "react": "^19.0.1",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.1",
    "resend": "^4.0.0",
    "rxjs": "^7.8.1",
    "stripe": "^18.4.0",
    "tailwind-merge": "^2.2.1",
    "tslib": "^2.8.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@react-email/preview-server": "5.0.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.12",
    "@types/node": "20.11.5",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "~8.56.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "react-email": "5.0.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.3.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bcrypt"
    ]
  }
}
