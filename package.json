{
  "name": "@thon-labs-services/source",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "start:auth": "turbo run start --filter=auth-services",
    "build:auth": "turbo run build --filter=auth-services",
    "run:auth-cert": "CERT=1 pnpm run:auth",
    "run:auth": "PORT=3100 turbo run serve --filter=auth-services",
    "run:website": "turbo run serve --filter=website",
    "run:checkout": "stripe listen --forward-to localhost:3000/checkout/webhook",
    "run:email": "turbo run serve --filter=@thon-labs-services/react-email",
    "db": "turbo run prisma --filter=@thon-labs-services/database",
    "db:push": "turbo run push --filter=@thon-labs-services/database && pnpm db:generate",
    "db:create-migration": "turbo run create-migration --filter=@thon-labs-services/database",
    "db:generate": "turbo run generate --filter=@thon-labs-services/database",
    "db:migrate": "turbo run migrate --filter=@thon-labs-services/database && pnpm db:generate",
    "db:studio": "turbo run studio --filter=@thon-labs-services/database",
    "db:reset": "turbo run reset --filter=@thon-labs-services/database"
  },
  "private": true,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/throttler": "^5.1.1",
    "@prisma/client": "^5.8.1",
    "@react-email/components": "^0.0.14",
    "@react-email/render": "^0.0.12",
    "@react-email/tailwind": "^0.0.14",
    "axios": "^1.12.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.3.0",
    "date-fns-tz": "^3.1.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
